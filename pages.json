{
	"easycom": {
		// uview 组件
		"^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue",
		// 自定义组件
		"^cl-(.*)": "@/components/cl-$1/cl-$1.vue"
	},

	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"name": "index",
			"aliasPath": "/",
			"meta": {
				"title": "首页"
			},
			"style": {
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/games/gameMenu",
			"name": "gameMenu",
			"aliasPath": "/gameMenu",
			"meta": {
				"title": "游戏目录"
			},
			"style": {
				"navigationStyle": "custom",
				"navigationBarTitleText": "游戏目录"
			}
		}, {
			"path": "pages/games/gameBegin",
			"name": "gameBegin",
			"aliasPath": "/gameBegin",
			"meta": {
				"title": "游戏开始"
			},
			"style": {
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/index/logo",
			"name": "logo",
			"aliasPath": "/logo",
			"meta": {
				"title": "游戏开始"
			},
			"style": {
				"navigationStyle": "custom"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "猜四个数字",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}